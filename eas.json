{
  "cli": {
    "version": ">= 10.2.1",
    "appVersionSource": "remote"
  },
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      },
      "preview2": {
        "android": {
          "gradleCommand": ":app:assembleRelease"
        }
      },
      "preview3":{
        "developmentClient":true
      },
      "production": {
        "autoIncrement": true
      }
    }
      
  }
